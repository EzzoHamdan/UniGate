@model UniversityGate.Models.Student

@{
    ViewBag.Title = "Create";
}

<h2>Add New Student</h2>

@using (Html.BeginForm("Add","Students",FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="form-horizontal">
        @Html.Partial("_InputFiled")

        <input type="submit" value="ADD" class="btn btn-outline-success" />
    </div>
}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
